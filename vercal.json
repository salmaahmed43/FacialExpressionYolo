{
    "builds": [{
        "src": "FacialExpressionYolo/main.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"FacialExpressionYolo/main.py"
        }
    ],
    "rewrites": [
        {
            "src": "/(.*)",
            "dest":"/"
        }
    ]
}
